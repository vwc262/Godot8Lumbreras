; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cutzamala"
run/main_scene="res://Scenes/Perfil/Main/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Cutzamala"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://Icon/App_icono.png"

[autoload]

NavigationManager="*res://Scripts/Managers/NavigationManager.gd"
GlobalData="*res://Scripts/Managers/Global_DataStore.gd"
TweenManager="*res://Scripts/Managers/Global_TweenManager.gd"
GlobalUtils="*res://Scripts/Managers/Global_Utils.gd"
UIManager="*res://Scripts/Managers/Global_UIManager.gd"
SceneManager="*res://Scripts/Managers/Global_SceneManager.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
GlobalTextureResource="*res://Resources/GlobalTextureResrouce.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/subwindows/embed_subwindows=false
window/stretch/aspect="ignore"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/anthonyec.camera_preview/plugin.cfg", "res://addons/easy_charts/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[filesystem]

import/fbx/enabled=false

[input]

screenshoot={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194308,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.521141, 0.521141, 0.521141, 1)
